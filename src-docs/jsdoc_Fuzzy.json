{
    "plugins": [],
    "recurseDepth": 1,
    "source": {
		"include": [ "../src/JavaScript/FuzzyLogic.js" ],
        "includePattern": ".+\\.js$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "script",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "copyright"             : "Copyright (c) 2020-2022 Nicolas Dufresne and contributors",
        "includeDate"           : true,
        "search"                : "true"
    },
    "linenums": true,
	"meta": {
        "title": "DuFuzzyLogic",
        "description": "A Fuzzy Logic Implementation",
        "keyword": "Javascript, Python, Fuzzy Logic, Logic"
    },
	"opts": {
        "template": "node_modules/jaguarjs-jsdoc",
		"recurse": false,
        "destination": "../docs/js-reference"
    }
}
