{
    "plugins": [],
    "recurseDepth": 1,
    "source": {
		"include": [ "../FuzzyLogic.js" ],
        "includePattern": ".+\\.js$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "script",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "copyright"             : "Copyright (c) 2020 Nicolas Dufresne and contributors",
        "includeDate"           : true,
        "theme"                 : "slate",
        "collapseSymbols"       : "false",
        "syntaxTheme"           : "dark",
        "search"                : "true"
    },
	"opts": {
        "template": "node_modules/tui-jsdoc-template",
		"recurse": false,
        "destination": "../../../docs/js"
    }
}
